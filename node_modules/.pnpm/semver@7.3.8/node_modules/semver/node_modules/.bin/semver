#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/arick/Documents/Labs/Tutos/Web/JS/Back-end/Learning/crud-rest-api-with-postgres/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/arick/Documents/Labs/Tutos/Web/JS/Back-end/Learning/crud-rest-api-with-postgres/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/semver.js" "$@"
else
  exec node  "$basedir/../../bin/semver.js" "$@"
fi
